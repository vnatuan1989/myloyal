{% extends '@nucleus/partials/particle.html.twig' %}

{% block particle %}
<a href="{{ url(particle.url)|e }}" title="{{ particle.alt|e }}" rel="{{ particle.rel|e }}" class="{{ particle.class|e }}">
    {% if particle.image %}
    <img src="{{ url(particle.image)|e }}" alt="{{ particle.alt|e }}" />
    {% endif %}
    {{ particle.text|e }}
</a>
{% endblock %}
